<Window x:Class="MhW.Wpf.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:regions="http://prismlibrary.com/"
        Title="MainWindow" Height="350" Width="525">
  <Grid Background="BlanchedAlmond">
    <StackPanel>
      <Button Content="Navigate to MonsterView" Command="{Binding NavigateCommand}" CommandParameter="MonsterView" />
      <Button Content="Navigate to GearManagementView" Command="{Binding NavigateCommand}" CommandParameter="GearManagementView" />
      <ContentControl regions:RegionManager.RegionName="ContentRegion" />
    </StackPanel>
  </Grid>
</Window>